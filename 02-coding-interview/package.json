{
    "name": "online-coding-interview",
    "version": "1.0.0",
    "description": "Online Coding Interview Platform",
    "scripts": {
        "client": "npm run dev --prefix client",
        "server": "npm run dev --prefix server",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "install_all": "npm install && npm --prefix client install && npm --prefix server install"
    },
    "devDependencies": {
        "concurrently": "^8.2.1"
    }
}